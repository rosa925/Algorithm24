    "def factorial_iter(n) :\n",
    "    result = 1\n",
    "    for k in range(1, n+1) :\n",
    "        result = result * k\n",
    "    return result\n",
    "\n",
    "print(f\"8!는 {factorial_iter(10)}\")"
   ]
  }
