    "def sequential_search(A, key):\n",
    "    for i in range(len(A)):\n",
    "        if A[i] == key:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "result = sequential_search([5,3,8,4,2,7], 1)\n",
    "print(result)"
   ]
