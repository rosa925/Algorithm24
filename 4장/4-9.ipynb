    "def powerMat(x, n) :\n",
    "    if n == 1 :\n",
    "        return x\n",
    "    elif (n%2) == 0 :\n",
    "        return powerMat(multMat(x,x), n // 2)\n",
    "    else :\n",
    "        return multMat(x, powerMat(multMat(x,x), (n - 1) // 2))\n",
    "    \n",
    "x = 2.0\n",
    "n = 10\n",
    "\n",
    "result = powerMat(x, n)\n",
    "\n",
    "print(f\"{x}의 {n} 거듭제곱은 {result}입니다.\")"
   ]
  }
