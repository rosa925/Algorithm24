    "def string_matching(T, P):\n",
    "    n = len(T)\n",
    "    m = len(P)\n",
    "    for i in range(n-m+1)\n",
    "    j = 0\n",
    "    while j < m and P[j]==T[i+j] :\n",
    "        j = j + 1\n",
    "    if j == m :\n",
    "        return i\n",
    "    return -1"
   ]
